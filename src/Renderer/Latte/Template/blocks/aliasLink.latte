{varType ApiGen\Index\Index $index}
{varType ApiGen\Renderer\Latte\Template\ConfigParameters $config}
{varType ApiGen\Renderer\Latte\Template\LayoutParameters $layout}

{define aliasLink, ApiGen\Info\ClassLikeInfo $classLike, ApiGen\Info\AliasInfo $alias, ?string $text = null}
	{pre}
		<a n:tag-if="classLikePageExists($classLike)" href="{aliasUrl($classLike, $alias)}">
			{if $text}{$text}{else}{$alias->name}{/if}
		</a>
	{/pre}
{/define}
