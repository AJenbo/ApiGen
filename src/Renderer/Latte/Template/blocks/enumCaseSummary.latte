{varType ApiGenX\Index\Index $index}
{varType ApiGenX\Renderer\Latte\Template\ConfigParameters $config}
{varType ApiGenX\Renderer\Latte\Template\LayoutParameters $layout}

{define enumCaseSummary, ApiGenX\Info\EnumInfo $enum}
	{if $enum->cases}
		<table class="table" id="cases">
			<tr>
				<th class="table-heading sortable" colspan="2" title="Switch between natural and alphabetical order">Cases</th>
			</tr>

			{foreach $enum->cases as $case}
				{include enumCase, enum: $enum, enumCase: $case}
			{/foreach}
		</table>
	{/if}
{/define}
